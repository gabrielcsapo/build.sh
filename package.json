{
  "name": "build.sh",
  "version": "1.0.3",
  "description": "ðŸ”¨ run and visualize the build process",
  "repository": {
    "type": "https",
    "url": "https://github.com/gabrielcsapo/build.sh.git"
  },
  "bugs": {
    "url": "https://github.com/gabrielcsapo/build.sh/issues"
  },
  "homepage": "https://www.gabrielcsapo.com/build.sh",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "tape test/**.js",
    "coverage": "tap test --coverage --coverage-report=lcov --no-browser",
    "generate-docs": "tryitout && jsdoc -c jsdoc.json",
    "pack": "pkg bin/build.js -c package.json -o packed/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./docs/storybook"
  },
  "bin": {
    "build": "./bin/build.js"
  },
  "author": "Gabriel J. Csapo <gabecsapo@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "ascii-pipeline": "0.0.3",
    "async": "^2.6.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "commander": "^2.13.0",
    "css-loader": "^0.28.8",
    "html-webpack-inline-source-plugin": "0.0.9",
    "html-webpack-plugin": "^2.30.1",
    "js-yaml": "^3.10.0",
    "log-update": "^2.3.0",
    "opn": "^5.1.0",
    "prop-types": "^15.6.0",
    "psychic.css": "0.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.9.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.8",
    "@storybook/react": "^3.3.8",
    "docdash": "^0.4.0",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "jsdoc": "^3.5.5",
    "pkg": "^4.2.6",
    "tap": "^11.0.1",
    "tape": "^4.8.0",
    "tryitout": "^2.0.6"
  },
  "pkg": {
    "scripts": [
      "bin/*",
      "util/**/*"
    ],
    "assets": [
      "src/**/*",
      "node_modules/psychic.css/dist/psychic.min.css"
    ],
    "targets": [
      "node8-macos-x64",
      "node8-alpine-x64",
      "node8-linux-x64",
      "node8-win-x64"
    ]
  }
}
